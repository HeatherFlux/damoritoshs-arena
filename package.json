  },
  "dependencies": {
    "fuse.js": "^7.0.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260103.0",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.7.2",
    "vite": "^6.2.4",
    "vue-tsc": "^2.2.8",
    "wrangler": "^4.54.0"
  }
